# Copy to .env and set your keys. Used by Docker Compose and local backend.

# OpenAI (default)
OPENAI_API_KEY=

# Optional: Anthropic
ANTHROPIC_API_KEY=

# Optional: Google Gemini
GOOGLE_GENERATIVE_AI_API_KEY=

# Optional: Grok (xAI)
XAI_API_KEY=

# Optional: Ollama (local) â€“ base URL if not using default
OLLAMA_BASE_URL=

# Optional: Ollama Cloud (ollama.com)
OLLAMA_API_KEY=

# Default provider and model when not set per-game from the UI
DEFAULT_PROVIDER=openai
DEFAULT_MODEL=gpt-4o-mini
